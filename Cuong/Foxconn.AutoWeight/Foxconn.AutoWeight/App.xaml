<Application x:Class="Foxconn.AutoWeight.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Foxconn.AutoWeight"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Icons.xaml" />
                <ResourceDictionary Source="StylesCore.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Foxconn.UI;Component/Themes/ExpressionDark.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Foxconn.UI;Component/Themes/Icons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Foxconn.UI;Component/Themes/StylesSimple.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
